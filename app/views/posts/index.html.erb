<p style="color: green"><%= notice %></p>

<% content_for :title, "Posts" %>


<div id="posts">
  <% @posts.each do |post| %>
    <div class="post-container">
      <%= render post %>
        <%= link_to "Show this post", post %>
    </div>
  <% end %>
  
</div>

